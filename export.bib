	file = {arXiv Fulltext PDF:C\:\\Users\\csanchidrian\\Zotero\\storage\\WFAUZQXP\\Meyer et al. - 2018 - Digital Communication with Rydberg Atoms & Amplitu.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\csanchidrian\\Zotero\\storage\\PHP9UVKT\\1803.html:text/html},
