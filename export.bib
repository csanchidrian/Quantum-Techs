	year = {2020},
	note = {arXiv:1910.00646 [physics, physics:quant-ph]},
	keywords = {electric field sensing, Physics - Atomic Physics, Quantum Physics, quantum sensor, Rydberg atoms, wideband sensing},
	pages = {034001},
	annote = {Comment: 16 pages, 8 figures},
	file = {Meyer et al. - 2020 - Assessment of Rydberg Atoms for Wideband Electric .pdf:C\:\\Users\\csanchidrian\\Zotero\\storage\\Q2G26NY6\\Meyer et al. - 2020 - Assessment of Rydberg Atoms for Wideband Electric .pdf:application/pdf},
